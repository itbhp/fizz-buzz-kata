{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test jest --no-cache --watch",
    "build": "tsc",
    "lint:staged": "lint-staged && git add app",
    "start": "npm run build && node dist/source/main.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.13",
    "jest": "24.6.0",
    "lint-staged": "8.1.1",
    "ts-jest": "24.0.2",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "1.13.0",
    "typemoq": "2.1.0",
    "typescript": "3.2.2"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": "tslint --fix"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*Test.(ts|tsx)"
    ]
  }
}
